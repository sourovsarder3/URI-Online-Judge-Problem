#include<iostream>
#include<iomanip>
#include<math.h>
using namespace std;
int main()
{
    int X;
    double x1, y1, x2, y2, sqt, distance;
    cin>>x1>>y1;
    cin>>x2>>y2;
    sqt = (x2-x1)*(x2-x1) + (y2-y1)*(y2-y1);
    distance = sqrt(sqt);

    cout<<fixed<<setprecision(4)<<distance<<endl;
    return 0;
}
