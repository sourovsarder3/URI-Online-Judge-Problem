#include<iostream>
#include<iomanip>
using namespace std;
int main()
{
    int hundred=0, fifty=0, twenty=0, ten=0, five=0, two=0, one=0, half=0, quarter=0, tenPaisa=0, fivePaisa=0, onePaisa=0, X, Y, X2;
    double amount;
    cin>>fixed>>setprecision(2)>>amount;
    Y = amount*100;
    X = Y/100;
    Y = Y%100;
    hundred = X/100;
    X = X%100;
    fifty = X/50;
    X = X%50;
    twenty = X/20;
    X = X%20;
    ten = X/10;
    X = X%10;
    five = X/5;
    X = X%5;
    two = X/2;
    X = X%2;
    one = X;
    half = Y/50;
    Y = Y%50;
    quarter = Y/25;
    Y = Y%25;
    tenPaisa = Y/10;
    Y = Y%10;
    fivePaisa = Y/5;
    Y = Y%5;
    onePaisa = Y;


    cout<<"NOTAS:"<<endl;
    cout<<hundred<<" nota(s) de R$ 100.00"<<endl;
    cout<<fifty<<" nota(s) de R$ 50.00"<<endl;
    cout<<twenty<<" nota(s) de R$ 20.00"<<endl;
    cout<<ten<<" nota(s) de R$ 10.00"<<endl;
    cout<<five<<" nota(s) de R$ 5.00"<<endl;
    cout<<two<<" nota(s) de R$ 2.00"<<endl;
    cout<<"MOEDAS:"<<endl;
    cout<<one<< " moeda(s) de R$ 1.00"<<endl;
    cout<<half<< " moeda(s) de R$ 0.50"<<endl;
    cout<<quarter<< " moeda(s) de R$ 0.25"<<endl;
    cout<<tenPaisa<< " moeda(s) de R$ 0.10"<<endl;
    cout<<fivePaisa<< " moeda(s) de R$ 0.05"<<endl;
    cout<<onePaisa<< " moeda(s) de R$ 0.01"<<endl;
    return 0;
}
