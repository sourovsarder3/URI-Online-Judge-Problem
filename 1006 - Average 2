#include<iostream>
#include<iomanip>
using namespace std;
int main()
{
    double A, B, C, MEDIA;
    cin>>A>>B>>C;
    A = A*2;
    B = B*3;
    C = C*5;
    MEDIA = (A+B+C)/10;
    cout<<fixed<<setprecision(1)<<"MEDIA = "<<MEDIA<<endl;
    return 0;
}
